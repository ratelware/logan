<?xml version="1.0"?>
<configuration xmlns="https://ratelware.com/api/logan/xsd">
    <grep_defaults>
      <is_regex>true</is_regex>
      <is_inverse>false</is_inverse>
      <is_case_sensitive>true</is_case_sensitive>
    </grep_defaults>
    <search_defaults>
        <is_regex>true</is_regex>
        <is_case_sensitive>true</is_case_sensitive>
        <is_reverse>false</is_reverse>
        <wrap_around>true</wrap_around>
    </search_defaults>
    <tabs_config>
        <root_tab_name>[base]</root_tab_name>
    </tabs_config>
    <colors>
        <generated_colors_config>
            <hue_min>0</hue_min>
            <hue_max>359</hue_max>
            <saturation_min>100</saturation_min>
            <saturation_max>150</saturation_max>
            <lightness_min>180</lightness_min>
            <lightness_max>255</lightness_max>
            <alpha_min>255</alpha_min>
            <alpha_max>255</alpha_max>
        </generated_colors_config>
        <selection_color>#D0F0C0</selection_color>
        <initial_emphasis_colors>
            <color>#FFAE9D</color>
            <color>#9DD5FF</color>
            <color>#9DFFC5</color>
        </initial_emphasis_colors>
    </colors>
    <icons_config>
        <mappings>
            <mapping>
                <regex>.*INFO.*</regex>
                <icon>:/icons/info.png</icon>
            </mapping>
            <mapping>
                <regex>.*INSERT INTO.*</regex>
                <icon>:/icons/lightning.png</icon>
            </mapping>
        </mappings>
        <favorites>
            <favorite>:/icons/exclaim.png</favorite>
            <favorite>:/icons/exclaim-question.png</favorite>
            <favorite>:/icons/question.png</favorite>
            <favorite>:/icons/fail.png</favorite>
            <favorite>:/icons/pass.png</favorite>
            <favorite>:/icons/lightning.png</favorite>
            <favorite>:/icons/bug.png</favorite>
            <favorite>:/icons/trophy.png</favorite>
        </favorites>
        <paths>
            <dir>:/icons/</dir>
        </paths>
    </icons_config>
</configuration>
